{
  "name": "ryoa-starter",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "dotenvx run -- turbo build",
    "dev": "dotenvx run -f .env -f .env.local -- turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "ui": "pnpm --filter @repo/ui ui",
    "create:migration": "turbo --filter @repo/server create:migration",
    "migrate": "turbo migrate",
    "migrate:dev": "turbo migrate:dev",
    "seed": "turbo seed",
    "seed:dev": "turbo seed:dev",
    "test:unit": "turbo test:unit"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.24.0",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "prettier": "catalog:",
    "turbo": "2.1.1"
  },
  "packageManager": "pnpm@9.12.0",
  "engines": {
    "node": ">=20.17.0"
  },
  "volta": {
    "node": "20.17.0",
    "pnpm": "9.12.0"
  }
}
